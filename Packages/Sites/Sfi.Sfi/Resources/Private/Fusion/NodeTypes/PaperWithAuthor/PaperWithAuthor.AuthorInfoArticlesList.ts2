prototype(Sfi.Sfi:PaperWithAuthor.AuthorInfoArticlesList) < prototype(Neos.Fusion:Collection) {
    @process.tmpl = ${'<div><h2 class="Service--Large">Другие материалы этого автора</h2>' + value + '</div>'}
    collection = ${q(site).find('[instanceof Sfi.Sfi:PaperWithAuthor]').get()}
    #collection.@process.filterOutSelf = ${q(value).filter('[_identifier != "' + node.identifier + '"]').get()}
    collection.@process.filterByAuthor = ${q(value).filterByReference('author', node.properties.author).get()}
    itemName = 'node'
    itemRenderer = Sfi.Sfi:PaperWithAuthor.TeaserForAuthorInfo
}
