	assetsPracticeAnnotations = Neos.Neos:ContentCollection {
		nodePath = 'assetsPracticeAnnotations'
		@process.title = ${'<h3 class="H3">' + Translation.translate('studyProgram.assetsPracticeAnnotations', null, [], null, 'Sfi.Sfi') + '</h3>' + value}
	}
