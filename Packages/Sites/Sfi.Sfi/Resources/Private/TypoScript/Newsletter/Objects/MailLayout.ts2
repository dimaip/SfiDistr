prototype(Sfi.Sfi:MailLayout) < prototype(TYPO3.TypoScript:Template) {
    templatePath = 'resource://Sfi.Sfi/Private/TypoScript/Newsletter/Objects/MailLayout.html'
    value = ${value}

    logoUri = TYPO3.TypoScript:ResourceUri {
        path = 'resource://Sfi.Sfi/Public/Images/sfi-newsletter.png'
    }

    unsubscribeLink = Psmb.Newsletter:UnsubscribeLink
    editSubscriptionLink = Psmb.Newsletter:EditSubscriptionLink
}
