prototype(TYPO3.Neos:Content) {
	@process.applyTags = TYPO3.TypoScript:Tag {
		attributes.itemprop = ${q(node).property('itemprop') ? q(node).property('itemprop') : null}
		attributes.itemscope = ${q(node).property('itemtype') ? true : null}
		attributes.itemtype = ${q(node).property('itemtype') ? q(node).property('itemtype') : null}
		content = ${value}
	}
}
